# VoiceFlow Environment Variables
# Copy this file to .env and fill in your values

# Picovoice (required for wake-word detection)
# Get your key at: https://console.picovoice.ai/
PICOVOICE_ACCESS_KEY=your_picovoice_access_key_here

# Tailscale Remote Access (optional)
# Used for remote control via iOS Shortcuts
VOICEFLOW_BEARER_TOKEN=your_secure_bearer_token_here

# Pushover Notifications (optional)
# Get keys at: https://pushover.net/
PUSHOVER_USER_KEY=your_pushover_user_key_here
PUSHOVER_API_TOKEN=your_pushover_api_token_here
